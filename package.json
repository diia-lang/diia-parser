{
  "name": "diia-parser",
  "version": "0.0.31",
  "description": "Diia language parser.",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "start": "node src/main.js",
    "antlr4": "cd ./src/antlr4/ && java -jar ../../vendor/antlr-4.11.1-complete.jar -Dlanguage=JavaScript -o build *.g4 -visitor",
    "antlr4c": "cd ./src/antlr4/ && java -jar ../../vendor/antlr-4.11.1-complete.jar -Dlanguage=Cpp -o buildc *.g4 -visitor",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kohutd/diia-parser.git"
  },
  "keywords": [
    "diia",
    "parser",
    "antlr",
    "antlr4",
    "дія",
    "парсер"
  ],
  "author": "kohutd (davyd.kohut@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kohutd/diia-parser/issues"
  },
  "homepage": "https://github.com/kohutd/diia-parser#readme",
  "devDependencies": {
    "jest": "^29.2.1"
  },
  "dependencies": {
    "antlr4": "^4.11.0"
  }
}
